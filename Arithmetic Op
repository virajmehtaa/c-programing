#include<iostream>
using namespace std;

int main()
{
    int a = 6;
    int b = 3;
    
    //Arithmetic Op -> +,-,*,/,%
    
    int add = a + b;
    int sub = a - b;
    int mul = a * b;
    int d1  =  a / b;
    int mod = a % b;
    
    cout<<"\n Your Addition is "<< add;
    cout<<"\n Your Substraction is "<< sub;
    cout<<"\n Your Multiplication is "<< mul;
    cout<<"\n Your Division is "<< d1;
    cout<<"\n Your Modulus is "<< mod;
    
    return 0;
}
